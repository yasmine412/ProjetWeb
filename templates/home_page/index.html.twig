<body>



{% include 'header/index.html.twig' %}
{%  include 'subnav_carousel.html.twig' %}

{% set i = 1 %}
{% for logement in logements %}
    {% if i%4 == 1 %}
        <div class="grand_conteneur">
    {% endif %}
    <div  class="conteneur-annonce">
        <div class="images-maison">
            {%set img=controller.imageParId(logement.id) %}

            {% include 'carousel.html.twig' with {'numAnnonce': logement.id,'images':img} %}
        </div>
        {% set path= path('app_home_page') ~ '/rooms/' ~ logement.id %}
        <div style="cursor:pointer;" onclick="window.location.href='{{ path }}'" class="pays"> <div>{{ logement.ville }},{{logement.pays }} </div> <div><i class="bi bi-star-fill"></i>  <span class="note">5,0</span></div> </div>
        <div style="cursor:pointer;" onclick="window.location.href='{{ path }}'" class="titre">{{ logement.rue}}</div>
        <div style="cursor:pointer;" onclick="window.location.href='{{ path }}'" class="date">N° Logement {{ logement.id }}</div>
        <div style="cursor:pointer;" onclick="window.location.href='{{ path }}'" class="prix">{%set floatNumber=logement.prix_nuite %}
            {% set intNumber = floatNumber|round(0, 'floor') %}
            {{ intNumber }}$
            <span onclick="window.location.href='{{path }}'" class="nuité">par nuit</span></div>
    </div>
    {% if i%4 == 0 %}
        </div>
    {% endif %}
    {% set i = i + 1 %}
{% endfor %}




</body>
