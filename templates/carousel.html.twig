<div id="carouselExampleIndicators{{numAnnonce}}" class="carousel slide" data-ride="carousel" data-interval="false">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators{{numAnnonce}}" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators{{numAnnonce}}" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators{{numAnnonce}}" data-slide-to="2"></li>
        <li data-target="#carouselExampleIndicators{{numAnnonce}}" data-slide-to="3"></li>

    </ol>
    <div class="carousel-inner">
        {%set nbr=1%}

        {% for image in images %}


            {% if nbr==1 %}
        <div class="carousel-item active">
            <img src="data:image/png;base64,{{ image.data }}" alt="image not found">
        </div>

                {% else %}
                    {% if nbr<=4 %}
                    <div class="carousel-item">
                        <img src="data:image/png;base64,{{ image.data }}" alt="image not found">
                    </div>
                    {% endif %}
            {% endif %}
            {% set nbr=nbr+1 %}
        {% endfor %}

    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators{{numAnnonce}}" role="button" data-slide="prev">
        <i class="bi bi-arrow-left-circle-fill"></i>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators{{numAnnonce}}" role="button" data-slide="next">
        <i class="bi bi-arrow-right-circle-fill"></i>
    </a>
</div>
